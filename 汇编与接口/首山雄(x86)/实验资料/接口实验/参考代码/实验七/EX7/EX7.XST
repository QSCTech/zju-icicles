set -tmpdir "./xst/projnav.tmp"
set -xsthdpdir "./xst"
run
-ifn ex7.prj
-ifmt mixed
-ofn ex7
-ofmt NGC
-p xc3s500e-5-fg320
-top ex7
-opt_mode Speed
-opt_level 1
