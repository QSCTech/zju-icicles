DESIGN mini4 ;
UNITS DISTANCE MICRONS 1000 ;

DIEAREA ( 0 0 ) ( 10000000 10000000 ) ;

ROW topin  IOSITE       0        0  N DO 9 BY  1 STEP 1000000 0 ;
ROW topout IOSITE 9000000        0  N DO 1 BY  1 STEP 1000000 0 ;
ROW botin  IOSITE 1000000 10000000 FN DO 9 BY  1 STEP 1000000 0 ;
ROW botout IOSITE 9000000 10000000 FN DO 1 BY  1 STEP 1000000 0 ;
ROW rowa ORDINARY       0  3000000  N DO 10 BY 1 STEP 1000000 0 ;
ROW rowb ORDINARY       0  4000000  N DO 10 BY 1 STEP 1000000 0 ;
ROW rowc ORDINARY       0  5000000  N DO 10 BY 1 STEP 1000000 0 ;
ROW rowd ORDINARY       0  6000000  N DO 10 BY 1 STEP 1000000 0 ;
ROW rowe ORDINARY       0  7000000  N DO 10 BY 1 STEP 1000000 0 ;

COMPONENTS 12 ;

- NIN1 INPAD + FIXED ( 1000000 0 ) N ;
- NIN2 INPAD + FIXED ( 5000000 0 ) N ;
- NOUT1 OUTPAD + FIXED ( 7000000 0 ) N ;
- NOR1 NORGATE ;
- NOR2 NORGATE ;
- NOR3 NORGATE ;
- NOR5 NORGATE ;
- NOR6 NORGATE ;
- NOR7 NORGATE ;
- SIN1 INPAD + FIXED ( 2000000 9000000 ) FN ;
- SIN2 INPAD + FIXED ( 4000000 9000000 ) FN ;
- SOUT1 OUTPAD + FIXED ( 1000000 9000000 ) FN ;

END COMPONENTS

SPECIALNETS 1 ;

- POWERNET ( NIN1 POWERPIN ) ( NIN2 POWERPIN ) ( NOUT1 POWERPIN )
           ( NOR1 POWERPIN ) ( NOR2 POWERPIN ) ( NOR3 POWERPIN )
           ( NOR5 POWERPIN ) ( NOR6 POWERPIN ) ( NOR7 POWERPIN )
           ( SIN1 POWERPIN ) ( SIN2 POWERPIN ) ( SOUT1 POWERPIN ) ;

END SPECIALNETS

NETS 14 ;

- N11 ( NIN1 OUTPIN ) ( NOR1 IN1 ) ;
- N11X ( NOR1 IN2 ) ;                #this is supposed to hook up to
                                     #an external pin
- N22 ( NIN2 OUTPIN ) ( NOR2 IN1 ) ;
- N22X ( NOR2 IN2 ) ;
- N71 ( NOR7 RESULT ) ( NOUT1 INPIN ) ;
- N13 ( NOR1 RESULT ) ( NOR3 IN1 ) ;
- N23 ( NOR2 RESULT ) ( NOR3 IN2 ) ;
- N31 ( NOR3 RESULT ) ( SOUT1 INPIN ) ;
- N57 ( NOR5 RESULT ) ( NOR7 IN1 ) ;
- N67 ( NOR6 RESULT ) ( NOR7 IN2 ) ;
- N15 ( SIN1 OUTPIN ) ( NOR5 IN1 ) ;
- N15X ( NOR5 IN2 ) ;
- N26 ( SIN2 OUTPIN ) ( NOR6 IN1 ) ;
- N26X ( NOR6 IN2 ) ;

END NETS

GROUPS 2 ;

- DOWNWARD NOR1 NOR2 NOR3
   + REGION ( 1000000 1000000 ) ( 5000000 5000000 ) ;
- UPWARD NOR5 NOR6 NOR7
   + REGION ( 5000000 5000000 ) ( 9000000 9000000 ) ;

END GROUPS

TRACKS Y 0 DO 1058 STEP 240 LAYER M1 M2 ;
TRACKS Y 0 DO 794 STEP 320 LAYER M2 M3 ;
TRACKS X 0 DO 1023 STEP 260 LAYER M1 M2 M3 ;

PINS 4 ;

- NEXP1 + NET N11X + DIRECTION INPUT + FIXED ( 0 4000000 ) N + LAYER M1 ( 0 0 ) ( 25 25 ) ;
- NEXP2 + NET N22X + DIRECTION INPUT + FIXED ( 0 6000000 ) N + LAYER M1 ( 0 0 ) ( 25 25 ) ;
- SEXP1 + NET N15X + DIRECTION INPUT + FIXED ( 10000000 7000000 ) FN + LAYER M1 ( 0 0 ) ( 25 25 ) ;
- SEXP2 + NET N26X + DIRECTION INPUT + FIXED ( 10000000 9000000 ) FN + LAYER M1 ( 0 0 ) ( 25 25 ) ;

END PINS


